<% for ( var i = 0; i < suppliers.length; i++){ %>
<div class="panel panel-default">
    <div class="panel-heading">
        <div class="panel-title"> <%= suppliers[i].name %></div>
    </div>
    <div class="panel-body">
        <p>Razón social: <b><%= suppliers[i].identifier_legalname %></b></p>
        <p>RFC: <b><%= suppliers[i].identifier_id %></b></p>
        <p>Sitio web: <b><%= suppliers[i].identifier_uri %></b></p>
        <a href="#" class="btn btn-default">Ver más</a>
    </div>
</div>
<%}%>